prefix=
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include/skia

Name: skia
Description: Skia is a complete 2D graphic library for drawing Text, Geometries, and Images.
Version: @VERSION@
Requires: expat, libpng, zlib, libwebp, libjpeg, harfbuzz, icu-uc, icu-i18n
Libs: -L${libdir} -lskia
Cflags: -I${includedir} @CFLAGS@
